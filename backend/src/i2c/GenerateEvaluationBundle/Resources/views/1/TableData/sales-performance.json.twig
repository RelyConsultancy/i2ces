[
    {% set prepend_comma = false %}
    {% for data in table_data %}
        {% if prepend_comma == true %}
            ,
        {% endif %}
        {
            "type": "{{ data.metric }}",
            "label": "{{ data.product }}",
            "value": "{{ data.results }}"
        }
        {% set prepend_comma = true %}
    {% endfor %}
]